{
	"editor.defaultFormatter": "astro-build.astro-vscode",
	"editor.formatOnPaste": true,
	"editor.formatOnSave": true,
	"cSpell.enableFiletypes": [
		"mdx"
	],
	"spellright.language": [
		"de",
		"en"
	],
	"spellright.documentTypes": [
		"markdown",
		"latex",
		"plaintext",
		"mdx"
	],
	"spellright.parserByClass": {
		"mdx": {
			"parser": "markdown"
		}
	}
}